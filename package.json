{
  "name": "dictionary-app",
  "version": "1.0.0",
  "private": true,
  "description": "Dictionary App",
  "author": "Sanchit Bhatnagar",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "prestart": "npm run clean",
    "build": "gatsby build",
    "predevbuild": "npm run clean",
    "devbuild": "npm run build",
    "postdevbuild": "npm run serve",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "^3.9.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-sass": "^4.9.0",
    "gatsby-plugin-sitemap": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "husky": "6.0 .0",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.10",
    "eslint": "^7.29.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.3.4",
    "@types/reach__router": "^1.3.8"
  }
}
